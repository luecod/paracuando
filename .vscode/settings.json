{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.organizeImports": true
  },
  "cSpell.words": [
    "commitlint",
    "esnext",
    "Hagámoslo",
    "jose",
    "ladygaga",
    "nextjs",
    "publicacion",
    "reduxjs",
    "Roboto"
  ],
  "cSpell.ignoreWords": ["Escríbela", "luisvalladolid", "paracuando"]
}
